{
  "name": "sample-admin",
  "type": "module",
  "version": "1.3.11",
  "description": "一个基于Vue3、Vite3、TypeScript、ElementPlus and UnoCSS的清新优雅的中后台模版。",
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite5",
    "TypeScript",
    "element-plus",
    "element-plus-admin",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "eslint --fix --ext .ts,.js,.jsx,.vue .",
    "postbuild": "sa print-soybean",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "4.1.2",
    "@sa/alova": "workspace:*",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@vueuse/components": "11.2.0",
    "@vueuse/core": "11.2.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "defu": "^6.1.4",
    "echarts": "5.5.1",
    "element-plus": "^2.8.7",
    "json5": "2.2.3",
    "nprogress": "0.2.0",
    "pinia": "2.2.6",
    "print-js": "1.6.0",
    "tailwind-merge": "2.5.4",
    "vue": "3.5.13",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "10.0.4",
    "vue-router": "4.4.5",
    "wangeditor": "4.7.15",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@elegant-router/vue": "0.3.8",
    "@iconify/json": "2.2.273",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.4.2",
    "@types/bmapgl": "0.0.7",
    "@types/dompurify": "3.0.5",
    "@types/node": "22.9.0",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "0.64.1",
    "@unocss/preset-icons": "0.64.1",
    "@unocss/preset-uno": "0.64.1",
    "@unocss/transformer-directives": "0.64.1",
    "@unocss/transformer-variant-group": "0.64.1",
    "@unocss/vite": "0.64.1",
    "@vitejs/plugin-vue": "5.2.0",
    "@vitejs/plugin-vue-jsx": "4.1.0",
    "eslint": "9.14.0",
    "eslint-plugin-vue": "9.31.0",
    "lint-staged": "15.2.10",
    "sass": "1.83.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-icons": "0.20.1",
    "unplugin-vue-components": "0.27.4",
    "vite": "5.4.11",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.6.4",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.1.10"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix"
  }
}
